# Copy to .env and fill in your keys
# .env is in .gitignore and will not be committed

# Groq — free tier, used for Graphiti entity extraction
GROQ_API_KEY=gsk_your_key_here

# Voyage AI — free tier, used for embeddings
VOYAGE_API_KEY=pa-your_key_here

# Neo4j Desktop — default local connection
NEO4J_URI=bolt://127.0.0.1:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password

# OLMoE model path (4-bit quantized)
OLMOE_MODEL_PATH=/Users/rajesh/athena/models/olmoe-4bit
